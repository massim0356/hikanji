<div class="container search">
  <%= render 'shared/searchbar' %>
</div>
<% if @results == 'No results found' || @results_readings == 'No results found' %>
  <h1>No results found</h1>
<% else %>
  <% if @switch %>
    <%= render 'kanjis/results', @results %>
  <% elsif @switch2 %>
    <%= render 'kanjis/results_reading', @results_readings %>
  <% end %>
<% end %>
